<h1>Display your Pirate here</h1>
<div class="pirate">
    <h2><%=@pirate.name%></h2>
    <h3><%=@pirate.weight%></h3>
    <h3><%=@pirate.height%></h3>
</div>

<h2>Display your first ship here</h2>
<div class="ship">
    <h2><%=@ships[0].name%></h2>
    <h3><%=@ships[0].type%></h3>
    <h3><%=@ships[0].booty%></h3>
</div>

<h2>Display your second ship here</h2>
<div class="ship">
    <h2><%=@ships[1].name%></h2>
    <h3><%=@ships[1].type%></h3>
    <h3><%=@ships[1].booty%></h3>
</div>